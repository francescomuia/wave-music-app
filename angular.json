    "wave-music-app": {
      "projectType": "application",
      "root": "apps/wave-music-app",
      "sourceRoot": "apps/wave-music-app/src",
      "prefix": "avr",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/wave-music-app",
            "index": "apps/wave-music-app/src/index.html",
            "main": "apps/wave-music-app/src/main.ts",
            "polyfills": "apps/wave-music-app/src/polyfills.ts",
            "tsConfig": "apps/wave-music-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/wave-music-app/src/favicon.ico",
              "apps/wave-music-app/src/assets"
            ],
            "styles": [
              "node_modules/@videogular/ngx-videogular/fonts/videogular.css",
              "apps/wave-music-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/wave-music-app/src/environments/environment.ts",
                  "with": "apps/wave-music-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "wave-music-app:build:production"
            },
            "development": {
              "browserTarget": "wave-music-app:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "wave-music-app:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/wave-music-app/src/**/*.ts",
              "apps/wave-music-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/wave-music-app"],
          "options": {
            "jestConfig": "apps/wave-music-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "wave-music-app-e2e": {
      "root": "apps/wave-music-app-e2e",
      "sourceRoot": "apps/wave-music-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/wave-music-app-e2e/cypress.json",
            "devServerTarget": "wave-music-app:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "wave-music-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/wave-music-app-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["wave-music-app"]
    }
